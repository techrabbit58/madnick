// Take a positive integer number greater than zero as the number of
// numbers from the Fibonacci series, with 1 as first, 1 as second,
// 2 as third and so forth.

        lda one
        sta a
        sta b

        inp
        brz range_error
        sta n

loop    lda n
        sub one
        sta n
        brp output
        hlt

output  lda a
        out
        sta aux
        lda b
        sta a
        add aux
        sta b
        bra loop

        // in case of a range error, answer "-1" (10's komplement)
range_error lda one
        sub one
        sub one
        out

        org 80
        // variables
a       dat
b       dat
aux     dat
n       dat

        org 90
        // constants
one     dat 1
